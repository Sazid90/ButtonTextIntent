<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ccf0f9f6"
    tools:context="nsu.sazid.buttontextintent.Main6Activity">

    <Button
        android:text="Website"
        android:id="@+id/button104"
        android:gravity="center"
        android:textSize="40sp"
        android:textColor="#f9f7f7"
        android:layout_gravity="center"
        android:layout_marginTop="130dp"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:background="@drawable/button_shape"/>
    <Button
        android:text="E-mail"
        android:id="@+id/button103"
        android:gravity="center"
        android:textSize="40sp"
        android:textColor="#f9f7f7"
        android:background="@drawable/button_shape"
        android:layout_gravity="center"
        android:layout_marginTop="50dp"
        android:layout_width="250dp"
        android:layout_height="50dp" />

    <Button
        android:id="@+id/button105"
        android:text="Dial"
        android:textSize="40sp"
        android:gravity="center"
        android:background="@drawable/button_shape"
        android:textColor="#f9f7f7"
        android:layout_gravity="bottom|center"
        android:layout_marginBottom="30dp"
        android:layout_width="250dp"
        android:layout_height="50dp" />
    <Button
        android:id="@+id/button102"
        android:text="Phonebook"
        android:onClick="onClickReset"
        android:textSize="40sp"
        android:gravity="center"
        android:background="@drawable/button_shape"
        android:textColor="#f9f7f7"
        android:layout_gravity="center"
        android:layout_marginBottom="30dp"
        android:layout_width="250dp"
        android:layout_height="50dp" />

    <Button
        android:id="@+id/button101"
        android:text="Camera"
        android:onClick="onClickReset"
        android:textSize="40sp"
        android:gravity="center"
        android:background="@drawable/button_shape"
        android:textColor="#f9f7f7"
        android:layout_gravity="center"
        android:layout_marginBottom="110dp"
        android:layout_width="250dp"
        android:layout_height="50dp" />
    <TextView
        android:text="About us"
        android:textSize="30sp"
        android:textColor="#0e0e0e"
        android:layout_gravity="top"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</FrameLayout>
